{
  "name": "alleyinteractive/mantle-installer",
  "description": "Mantle Installer",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Sean Fisher",
      "email": "sean@alley.co"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "symfony/console": "^5.0",
    "symfony/process": "^5.0",
    "wp-cli/core-command": "^2.0",
    "wp-cli/wp-cli": "^2.4"
  },
  "require-dev": {
    "alleyinteractive/alley-coding-standards": "^0.2.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "src/class-install-command.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/mantle"
  ],
  "scripts": {
    "phpcs": "phpcs --standard=./phpcs.xml .",
    "phpunit": "phpunit"
  }
}
