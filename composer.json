{
    "name": "alleyinteractive/mantle-installer",
    "description": "Mantle Installer",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sean Fisher",
            "email": "sean@alley.co"
        }
    ],
    "require": {
        "php": "^7.3",
        "symfony/console": "^5.1",
        "symfony/process": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/class-install-command.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/mantle"
    ]
}
